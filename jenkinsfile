@Library("jenkinslibrary"@master) _

def build = new org.library.build()

String buildShell = "${env.buildShell}"
String buildType = "${env.buildType}"

pipeline {
    agent {
        node {
            label "build-1"
        }
    }
    
    stages{
        stage("Build") {
            steps {
                script{
                    build.Build(buildType, buildShell)
                }
            }
        }
    }
}
